model Favourite {
  id           Int      @id @default(autoincrement())
  sourceId     String   @map("source_id")
  classifiedId Int      @map("classified_id")
  createdAt    DateTime @default(now()) @map("created_at")

  @@unique([sourceId, classifiedId])
  @@index([sourceId])
  @@map("favourites")
}
